{
    "extends": "@vue/tsconfig/tsconfig.dom.json",
  "include": ["vite.config.ts", "vite-env.d.ts", "env.d.ts", "src/**/*", "src/**/*.vue", "src/**/*.ts"],
  "files": [],
  "compilerOptions": {
        "composite": true,
        "module": "ESNext",
        "target": "ES2016",
        "baseUrl": ".",
        "lib": ["DOM", "ESNext", "ES2018"],
        "types": ["node", "jquery"],
        "moduleResolution": "Node", 
        "verbatimModuleSyntax": true,
        "skipLibCheck": true,
        "paths": {
            "@/*": ["src/*"]
        },
    },
}
