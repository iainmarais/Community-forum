{
  "name": "forum",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "https": "vite --config vite.config.https.ts --host",
    "http": "vite --config vite.config.http.ts --host",
    "serve": "vue-tsc -b && vite build && vite preview --host",
    "build": "vue-tsc -b && vite build",
    "preview": "vite preview --host",
    "start:all": "concurrently \"npm run https\" \"npm run http\""
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^6.5.2",
    "@vue/tsconfig": "^0.5.1",
    "axios": "^1.7.2",
