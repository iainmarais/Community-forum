    "jquery": "^3.6.1",
    "pinia": "^2.1.7",
    "popper.js": "^1.16.1",
    "postcss-scss": "^4.0.5",
    "url": "^0.11.3",
    "vue": "^3.5.8",
    "vue-router": "^4.4.5"
  },
  "devDependencies": {
    "@microsoft/signalr": "^8.0.7",
    "@types/bootstrap": "^5.2.10",
    "@types/jquery": "^3.5.30",
    "@types/node": "^20.1.0",
    "@types/nprogress": "^0.2.3",
    "@vitejs/plugin-vue": "^5.0.5",
    "bootstrap": "^4.6.0",
    "bootstrap-icons": "^1.11.3",
    "concurrently": "^9.0.1",
    "less": "^4.2.0",
    "less-loader": "^12.2.0",
    "node": "^20.15.0",
    "nprogress": "^0.2.0",
    "popper": "^1.0.1",
    "sass": "^1.77.8",
    "sass-loader": "^14.2.1",
    "smoothscroll": "^0.4.0",
    "sweetalert2": "^11.6.13",
    "typescript": "^5.5.2",
    "vite": "^5.3.1",
    "vue-spinkit": "^2.0.0",
    "vue-spinner": "^1.0.4",
    "vue-toastification": "^2.0.0-rc.5",
    "vue-tsc": "^2.0.21",
    "vue3-toastify": "^0.2.2",
    "vuetify": "^3.7.2"
  }
